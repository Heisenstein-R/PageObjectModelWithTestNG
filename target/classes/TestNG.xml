<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Page Object Model Testing Using TestNG on Free CRM site"
	guice-stage="DEVELOPMENT">

	<listeners>
		<listener class-name="com.qa.ExtentReportListener.ExtentReportListener" />
		<listener class-name="com.qa.util.MyListener" />
	</listeners>


	<test name="FreeCRMTestCases" verbose="2">


		<classes>
			<class name="com.qa.testcases.LoginPageTest" />
			<class name="com.qa.testcases.HomePageTest" />
			<class name="com.qa.testcases.ContactsPageTest" />
		</classes>


	</test> <!--Default test -->
</suite> <!--Default suite -->